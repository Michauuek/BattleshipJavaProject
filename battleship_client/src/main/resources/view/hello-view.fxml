<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>


<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.Pane?>
<Pane id="rootPane" fx:controller="com.example.battleship_client.controller.HelloController"
      styleClass="main-panel"
      xmlns:fx="http://javafx.com/fxml">

    <Label text="First Name:" /> <TextField fx:id="firstNameField" prefColumnCount="30" />
    <Label text="Last Name:" />	<TextField fx:id="lastNameField" prefColumnCount="30" />

    <Button text="Say Hello" onAction="#sayHello"/>

    <Label fx:id="messageLabel" styleClass="hello-message" />

</Pane >